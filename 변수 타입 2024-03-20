package s21212_00;

public class Overflow {

	public static void main(String[] args) {
		
		int var1 = 2147483646;
		for(int i=0; i<5; i++) { //{ }를 5번 반복 실행
	  	    var1++;
			System.out.println("var1: " + var1);
		}
		
		byte var2 = 125;
		for(int i=0; i<5; i++) { //{ }를 5번 반복 실행
			var2++;
			System.out.println("var2: " + var2);
		}
		
    }
}

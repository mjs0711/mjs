package s21212_00;

public class S21212_01v8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int intVal1 = 100;
		int intVal2 = -100;
		double douVal1 = 3.14;
		double douVal2 = -3.14;
		
		int intResult1 = -intVal1;
		int intResult2 = -intVal2;
		double douResult1 = -douVal1;
		double douResult2 = -douVal2;
		
		System.out.println("int1:"+intResult1+" int2:"+intResult2);
		System.out.println("dou1:"+douResult1 +"dou2:"+douResult2);
				
		

	}

}

package mjs;

import java.util.Random;
import java.util.Scanner;

public class Mjs {
	public static void main(String[] args) {
		// 컴퓨터 숫자 3개를 결정
		Random rd = new Random();
		
		int 민 = rd.nextInt(3+1);
		int 지 = 0;
		
		while(민 != 지) { // 민이랑 지랑 다르다 
			지 = rd.nextInt(3+1);
		}
		
		int 성 = rd.nextInt(3+1);
		
		System.out.println(민);
		System.out.println(지);
		System.out.println(성);
		
		
	}
}

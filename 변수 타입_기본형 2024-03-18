package s21212_00;

public class S21200_01v1 {

	public static void main(String[] args) {
		int intNum1 = 0b1011;
		int intNum2 = 0206;
		int intNum3 = 365;
		int intNum4 = 0xb3;
		
		System.out.println("Num1"+intNum1);
		System.out.println("Num2"+intNum2);
		System.out.println("Num3"+intNum3);
		System.out.println("Num4"+intNum4);

	}

}

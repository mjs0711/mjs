package s21212_00;

public class S21212_03 {

	public static void main(String[] args) {
	      int input = 0;
	      
	      if (input == 0) {
	            System.out.println("a : 값은 0 입니다");
	      }
	      if (input != 0) {
	         System.out.println("a : 값은 0 입니다");
	      }
	      
	      //b값은 if else로 코딩합니다
	      if (input == 0) {
	         System.out.println("b : 값은 0 입니다");
	      } else {
	         System.out.println("b : 값은 0 입니다");
	      }
	}

}

package mjs;

import java.util.Scanner;

public class Mjs {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
        System.out.println("숫자 입력 해");
       
        
//		num % 10 => 123456 % 10 = 6
//		for(초기값; 조건; 증강식)        
        for(int num=sc.nextInt(); num>0; num/=10) {  //num이 0이 될떄까지
        	System.out.println(num % 10);
        	
        }
        
        
        
        
	}

}
